#include <stdio.h>

int main() {
    int arr[5];  // Declaring an array of size 5
    
    // Input elements
    for (int i = 0; i < 5; i++) {
        printf("Enter element %d: ", i + 1);
        scanf("%d", &arr[i]);
    }
    
    // Output elements
    printf("Array elements are: ");
    for (int i = 0; i < 5; i++) {
        printf("%d ", arr[i]);
    }

    return 0;
}
